{
  "name": "account-manager",
  "private": true,
  "version": "1.0.0",
  "description": "Компонент для управления учетными записями (Vue 3 + Naive UI)",
  "author": "Kanasimy",
  "homepage": "https://Kanasimy.github.io/AccountManager/",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit -p tsconfig.app.json && vite build",
    "deploy": "vue-tsc -p tsconfig.app.json --noEmit && vite build && gh-pages -d dist -t",
    "preview": "vite preview",
    "lint": "eslint \"src/**/*.{ts,js,vue}\"",
    "lint:fix": "eslint \"src/**/*.{ts,js,vue}\" --fix",
    "format": "prettier --write \"src/**/*.{ts,js,vue,json,css,scss,html,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,vue,json,css,scss,html,md}\""
  },
  "dependencies": {
    "naive-ui": "^2.43.1",
    "vue": "^3.5.22"
  },
  "devDependencies": {
    "@types/node": "^24.6.0",
    "@typescript-eslint/parser": "^8.46.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "^10.5.0",
    "gh-pages": "^6.3.0",
    "globals": "^16.4.0",
    "pinia": "^3.0.3",
    "prettier": "^3.6.2",
    "typescript": "~5.9.3",
    "vite": "^7.1.7",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.1.0"
  }
}
